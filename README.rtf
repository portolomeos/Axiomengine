\f0\b\fs36 \cf0 Example: Emergent Structure Formation\
\pard\pardeftab720\partightenfactor0

\f2\b0\fs26 \cf0 # Create toroidal grid of grains\
manifold = RelationalManifold()\
\
# Create 5x5 grid on torus\
for i in range(5):\
    for j in range(5):\
        # Convert grid to toroidal coordinates\
        theta = (i / 5) * 2 * math.pi\
        phi = (j / 5) * 2 * math.pi\
        \
        # Add grain with coordinates\
        grain_id = f"grain_\{i\}_\{j\}"\
        manifold.add_grain(grain_id, theta=theta, phi=phi)\
\
# Connect nearest neighbors\
for i in range(5):\
    for j in range(5):\
        current = f"grain_\{i\}_\{j\}"\
        \
        # Connect to right neighbor (with wrap-around)\
        right = f"grain_\{(i+1)%5\}_\{j\}"\
        manifold.connect_grains(current, right)\
        \
        # Connect to bottom neighbor (with wrap-around)\
        bottom = f"grain_\{i\}_\{(j+1)%5\}"\
        manifold.connect_grains(current, bottom)\
\
# Set seed points with high awareness\
manifold.grains["grain_0_0"].awareness = 1.0\
manifold.grains["grain_2_2"].awareness = 1.0\
\
# Set opposite grains\
manifold.set_opposite_grains("grain_0_0", "grain_2_2")\
\
# Run simulation\
state = SimulationState()\
engine = EnhancedContinuousDualityEngine(manifold, state)\
\
# Run for 200 steps\
for step in range(200):\
    engine.step()\
    state.update(manifold)\
    \
    if step % 50 == 0:\
        print(f"Step \{step\}/200: Time = \{manifold.time:.2f\}, Nodes = \{len(manifold.grains)\}")\
\pard\pardeftab720\sa298\partightenfactor0

